<p><% if @operator == 'add' %>
    <%= @number1.to_i + @number2.to_i %>
    <% end %>
    <% if @operator == 'divide' %>
    <%= @number1.to_i / @number2.to_i %>
    <% end %>
    <% if @operator == 'subtract' %>
    <%= @number1.to_i - @number2.to_i %>
    <% end %>
    <% if @operator == 'multiply' %>
    <%= @number1.to_i * @number2.to_i %>
    <% end %>
</p>
